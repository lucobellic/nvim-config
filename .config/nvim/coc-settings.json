{
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "diagnostic.errorSign": "•",
  "diagnostic.hintSign": "•",
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.locationlistUpdate": true,
  "diagnostic.enableSign": true,
  "diagnostic.signPriority": 20,
  "diagnostic.enableMessage": "jump",
  "diagnostic.showUnused": true,
  "diagnostic.level": "information",
  "diagnostic.showDeprecated": true,
  "diagnostic.refreshOnInsertMode": true,
  "suggest.autoTrigger": "none",
  "snippets.textmateSnippetsRoots": [
    "~/.config/snippets"
  ],
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.file.column.indent.indentLine": false,
  "explorer.file.reveal.whenOpen": true,
  "python.formatting.provider": "yapf",
  "python.analysis.useLibraryCodeForTypes": false,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "semanticTokens.filetypes": ["*"],
  "clangd.enabled": true,
  "clangd.arguments": [
    "--background-index",
    "--clang-tidy",
    "--enable-config"
  ],
  "clangd.path": "C:/Program Files/LLVM/bin/clangd.exe",
  "rust.clippy_preference": "on",
  "rust.all_features": true,
  "languageserver": {
    "vimls": {
      "command": "vim-language-server",
      "args": [
        "--stdio"
      ],
      "initializationOptions": {
        "iskeyword": "@,48-57,_,192-255,-#",
        "vimruntime": "",
        "runtimepath": "",
        "diagnostic": {
          "enable": true
        },
        "indexes": {
          "runtimepath": true,
          "gap": 100,
          "count": 3,
          "projectRootPatterns": [
            ".git",
            "autoload",
            "plugin"
          ]
        },
        "suggest": {
          "fromVimruntime": true,
          "fromRuntimepath": false
        }
      },
      "filetypes": [
        "vim"
      ]
    }
  }
}
